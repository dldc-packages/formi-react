{
  "name": "react-formi",
  "version": "1.0.0-9",
  "description": "React form hook",
  "keywords": [
    "react",
    "form"
  ],
  "homepage": "https://github.com/etienne-dldc/react-formi#readme",
  "bugs": {
    "url": "https://github.com/etienne-dldc/react-formi/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/etienne-dldc/react-formi.git"
  },
  "license": "MIT",
  "author": "Etienne Dldc <e.deladonchamps@gmail.com>",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --watch",
    "lint": "prettier src --check && eslint src && tsc --noEmit",
    "lint:fix": "prettier src --write && eslint src --fix",
    "release": "yarn run build && np",
    "test": "yarn run lint && jest --coverage",
    "test:cov:watch": "jest --coverage --watch --all",
    "test:run": "jest",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch"
  },
  "dependencies": {
    "suub": "^3.2.0",
    "use-sync-external-store": "^1.0.0-rc.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.4.1",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/use-sync-external-store": "^0.0.3",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "codecov": "^3.8.3",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "fs-extra": "^10.0.1",
    "jest": "^27.5.1",
    "np": "^7.6.1",
    "prettier": "^2.6.1",
    "react": "^17.0.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.4",
    "tslib": "^2.3.1",
    "tsup": "^5.12.1",
    "typescript": "^4.6.3",
    "zod": "^3.14.3"
  },
  "peerDependencies": {
    "react": ">=17",
    "react-dom": ">=17",
    "zod": ">=3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
