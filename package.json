{
  "name": "react-formi",
  "version": "1.0.0-14",
  "description": "React form hook",
  "keywords": [
    "react",
    "form"
  ],
  "homepage": "https://github.com/etienne-dldc/react-formi#readme",
  "bugs": {
    "url": "https://github.com/etienne-dldc/react-formi/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/etienne-dldc/react-formi.git"
  },
  "license": "MIT",
  "author": "Etienne Dldc <e.deladonchamps@gmail.com>",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/mod.mjs",
      "require": "./dist/mod.js",
      "types": "./dist/mod.d.ts"
    }
  },
  "main": "./dist/mod.js",
  "module": "./dist/mod.mjs",
  "types": "./dist/mod.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && tsup --format cjs,esm src/mod.ts --dts src/mod.ts",
    "example:run": "vite example",
    "lint": "prettier . --check && eslint . && tsc --noEmit",
    "lint:fix": "prettier . --write . && eslint . --fix",
    "release": "release-it",
    "test": "pnpm run lint && vitest run --coverage",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "typecheck": "tsc",
    "typecheck:watch": "tsc --watch"
  },
  "dependencies": {
    "erreur": "^1.1.1",
    "suub": "^3.2.1",
    "use-sync-external-store": "^1.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "@types/use-sync-external-store": "^0.0.3",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "@vitest/coverage-v8": "^0.32.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "release-it": "^15.11.0",
    "rimraf": "^5.0.1",
    "tslib": "^2.5.3",
    "tsup": "^6.7.0",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vitest": "^0.32.0",
    "zod": "^3.21.4"
  },
  "peerDependencies": {
    "react": ">=17",
    "react-dom": ">=17",
    "zod": ">=3"
  },
  "packageManager": "pnpm@8.6.1",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "release-it": {
    "hooks": {
      "before:init": [
        "pnpm run build",
        "pnpm test"
      ]
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true,
      "web": true,
      "autoGenerate": true
    }
  }
}
