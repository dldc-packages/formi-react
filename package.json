{
  "name": "react-formi",
  "version": "1.0.0-1",
  "description": "React form hook",
  "keywords": [
    "react",
    "form"
  ],
  "homepage": "https://github.com/etienne-dldc/react-formi#readme",
  "bugs": {
    "url": "https://github.com/etienne-dldc/react-formi/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/etienne-dldc/react-formi.git"
  },
  "license": "MIT",
  "author": "Etienne Dldc <e.deladonchamps@gmail.com>",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/react-formi.es.js",
      "require": "./dist/react-formi.cjs.js"
    }
  },
  "main": "./dist/react-formi.cjs.js",
  "module": "./dist/react-formi.es.js",
  "types": "./dist/mod.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && vite build && yarn build:types",
    "build:types": "tsc -p ./tsconfig.build.json",
    "codecov": "codecov",
    "lint": "prettier . --check && eslint . && tsc --noEmit",
    "lint:fix": "prettier . --write . && eslint . --fix",
    "release": "yarn build && np",
    "test": "yarn lint && jest --coverage",
    "test:run": "jest",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "suub": "^3.1.3",
    "use-sync-external-store": "0.0.0-experimental-95d762e40-20210908"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.12",
    "@types/jest": "^27.0.1",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "codecov": "^3.8.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "fs-extra": "^10.0.0",
    "jest": "^27.1.1",
    "jest-ts-tsx-resolver": "^1.0.0",
    "np": "^7.5.0",
    "prettier": "^2.3.1",
    "react": "^17.0.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.4",
    "tslib": "^2.3.1",
    "typescript": "^4.3.5",
    "vite": "^2.5.6",
    "zod": "^3.8.2"
  },
  "peerDependencies": {
    "react": ">=16.8",
    "react-dom": ">=16.8",
    "zod": ">=3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
